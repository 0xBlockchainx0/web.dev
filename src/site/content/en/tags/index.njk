---
layout: layout
title: Tags
permalink: /en/tags/{{ paged.tag }}/{% if paged.index > 0 %}{{ paged.index + 1 }}/{% endif %}index.html
override:tags: []
description: Our latest news, updates, and stories for developers
date: 2018-11-05
renderData:
  title: "{{ paged.title or title }}"
  description: "{{ paged.description or description }}"
pagination:
  data: collections.pagedTags
  size: 1
  alias: paged
---

{# *-landing-page classes are a holdover from devsite. #}
{# It would be great to remove them as part of the v1 migration. #}
<div class="blog-landing-page">
  {# Make sure Actions are first in the tab order. #}
  <web-actions actions="subscribe"></web-actions>

  <div class="w-layout-container w-pb--non">
    <header class="w-page-header">
      <h1 class="w-page-header__headline">{{ renderData.title }}</h1>
      <p class="w-page-header__copy">{{ renderData.description }}</p>
      <div class="w-chips">
      </div>
    </header>
  </div>
  {% include 'partials/paged.njk' %}
</div>
